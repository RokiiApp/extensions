{
  "name": "rokii-extensions",
  "private": true,
  "description": "Extensions for Rokii app",
  "workspaces": [
    "extensions/*"
  ],
  "scripts": {
    "lint": "turbo lint",
    "build": "turbo build",
    "prepare-publish": "turbo run lint build"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "7.2.0",
    "@typescript-eslint/parser": "7.2.0",
    "eslint": "8.57.0",
    "semistandard": "17.0.0",
    "standard": "17.1.0",
    "turbo": "1.12.5",
    "typescript": "5.4.2"
  },
  "pnpm": {
    "overrides": {
      "react": "18.2.0",
      "react-dom": "18.2.0",
      "@types/react": "18.2.64",
      "typescript": "$typescript",
      "@rokii/build": "2.0.13",
      "@rokii/utils": "2.0.9",
      "@rokii/api": "0.8.1",
      "@tauri-apps/api": "2.0.0-beta.5",
      "@tauri-apps/cli": "2.0.0-beta.9",
      "@tauri-apps/plugin-fs": "2.0.0-beta.2",
      "@tauri-apps/plugin-updater": "2.0.0-beta.2",
      "@tauri-apps/plugin-dialog": "2.0.0-beta.2",
      "@tauri-apps/plugin-process": "2.0.0-beta.2",
      "@tauri-apps/plugin-shell": "2.0.0-beta.2",
      "@tauri-apps/plugin-autostart": "2.0.0-beta.2",
      "@tauri-apps/plugin-clipboard-manager": "2.0.0-beta.2",
      "@tauri-apps/plugin-global-shortcut": "2.0.0-beta.2",
      "@tauri-apps/plugin-http": "2.0.0-beta.2"
    }
  }
}

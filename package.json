{
  "name": "rokii-plugins",
  "private": true,
  "description": "Plugins for Rokii app",
  "workspaces": [
    "plugins/*"
  ],
  "scripts": {
    "lint": "turbo lint",
    "build": "turbo build",
    "prepare-publish": "turbo run lint build"
  },
  "devDependencies": {
    "@changesets/cli": "2.27.1",
    "@typescript-eslint/eslint-plugin": "7.1.1",
    "@typescript-eslint/parser": "7.1.1",
    "eslint": "8.57.0",
    "semistandard": "17.0.0",
    "standard": "17.1.0",
    "turbo": "1.12.4",
    "typescript": "5.3.3"
  },
  "pnpm": {
    "overrides": {
      "react": "18.2.0",
      "react-dom": "18.2.0",
      "@types/react": "18.2.62",
      "typescript": "$typescript",
      "@rokii/build": "2.0.13",
      "@rokii/types": "latest",
      "@rokii/utils": "latest",
      "@rokii/ui": "latest",
      "@rokii/api": "0.4.1",
      "@tauri-apps/api": "1.5.3"
    }
  }
}
